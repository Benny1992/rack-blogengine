#!/usr/bin/env ruby
require 'rack/blogengine/command_line_interface' 

if ARGV[0]
  command = ARGV[0].to_sym
else
  command = "".to_sym
end

target = ARGV[1]
target = "" if !ARGV[1]

cli = Rack::Blogengine::CommandLineInterface.new

if target != ""
  cli.send(command, target)
else
  cli.send(command)
end
